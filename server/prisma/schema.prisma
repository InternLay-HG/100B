generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int     @id @default(autoincrement())
  email        String  @unique
  name         String
  password     String
  year         String
  branch       String
  refreshToken String?
  role         Role[]  @default([STUDENT])
}

enum Role {
  STUDENT
  TEACHER
  ADMIN
}
